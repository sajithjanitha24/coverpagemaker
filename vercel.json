{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/gate.html",
      "dest": "public/gate.html"
    },
    {
      "src": "/index.html",
      "dest": "public/index.html"
    },
    {
      "src": "/generate",
      "dest": "api/app.py"
    }
  ]
}

